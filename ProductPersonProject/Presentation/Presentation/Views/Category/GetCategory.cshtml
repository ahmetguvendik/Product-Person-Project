@using Domain.Entities
@model IQueryable<Category>

<table class="mt-4 table table-hover">
    <thead>
        <tr>

            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <th>@item.Id</th>
                <th>@item.Name</th>
            </tr>
        }

    </tbody>

</table>
